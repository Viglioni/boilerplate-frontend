{
  "name": "boilerplate-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build:server": "rm -rf  dist && tsc --project tsconfig.server.json",
    "build:next": "rm -rf  .next && NODE_ENV=production next build",
    "build": "npm run build:next && npm run build:server",
    "export": "npm run build && next export",
    "export:only": "next export",
    "start": "NODE_ENV=production node dist/server/index.js",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "axios": "^0.19.2",
    "es-check": "^5.1.0",
    "formik": "~2.1.4",
    "next": "~9.5.2",
    "next-compose-plugins": "~2.2.0",
    "next-optimized-images": "~2.6.0",
    "next-redux-wrapper": "~6.0.0",
    "next-transpile-modules": "~4.1.0",
    "ramda": "^0.27.0",
    "react": "~16.13.0",
    "react-csv": "^2.0.3",
    "react-dom": "~16.13.0",
    "react-dropzone": "^11.0.1",
    "react-intl": "~4.5.9",
    "react-redux": "~7.2.0",
    "redux": "~4.0.5",
    "redux-devtools-extension": "~2.13.8",
    "redux-thunk": "~2.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.5.1",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/material-ui": "^0.21.8",
    "@types/next": "~9.0.0",
    "@types/next-redux-wrapper": "~3.0.0",
    "@types/node": "~14.0.5",
    "@types/ramda": "^0.27.32",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.5",
    "@types/react-redux": "~7.1.3",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "~3.0.0",
    "@typescript-eslint/parser": "~3.0.0",
    "babel-eslint": "~10.1.0",
    "babel-plugin-module-resolver": "~4.0.0",
    "babel-plugin-react-intl": "~7.5.12",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-jsx-control-statements": "~2.2.1",
    "eslint-plugin-prettier": "~3.1.3",
    "eslint-plugin-react": "~7.20.0",
    "husky": "~4.2.3",
    "jest": "^26.0.1",
    "lint-staged": "^10.5.1",
    "nodemon": "~2.0.2",
    "npm": "^6.14.8",
    "plop": "^2.7.1",
    "prettier": "~2.0.5",
